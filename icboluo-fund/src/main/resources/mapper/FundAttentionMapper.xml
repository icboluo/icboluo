<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.icboluo.mapper.FundAttentionMapper">

    <resultMap id="FundAttentionMap" type="com.icboluo.entity.FundAttention">
        <result property="id" column="id" jdbcType="VARCHAR"/>
    </resultMap>
    <sql id="Base_Column_List">
        id    </sql>
    <!--查询单个-->
    <select id="queryById" resultMap="FundAttentionMap">
        select
        <include refid="Base_Column_List"/>
        from note.fund_attention
        where id = #{id}
    </select>

    <!--查询所有-->
    <select id="queryAll" resultMap="FundAttentionMap">
        select
        <include refid="Base_Column_List"/>
        from note.fund_attention
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAllByData" resultMap="FundAttentionMap">
        select
        <include refid="Base_Column_List"/>
        from note.fund_attention
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into note.fund_attention()
        values ()
    </insert>

    <insert id="insertSelective" keyProperty="id" useGeneratedKeys="true">
        insert into note.fund_attention
        <trim prefix="(" suffix=")" suffixOverrides=",">
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
        </trim>
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into note.fund_attention()
        values
        <foreach collection="list" item="entity" separator=",">
            ()
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into note.fund_attention()
        values
        <foreach collection="list" item="entity" separator=",">
            ()
        </foreach>
        on duplicate key update
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update note.fund_attention
        <set>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete
        from note.fund_attention
        where id = #{id}
    </delete>

</mapper>

